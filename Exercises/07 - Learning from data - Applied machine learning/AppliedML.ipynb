    "from sklearn.linear_model import LogisticRegression\n",
    "from sklearn.metrics import confusion_matrix, accuracy_score, precision_score, recall_score, f1_score\n",
    "\n",
    "model = LogisticRegression()\n",
    "model.fit(train_features_norm, y_train)\n",
    "y_pred = model.predict(test_features_norm)\n",
    "confusion_matrix_result  = confusion_matrix(y_test, y_pred)\n",
    "print(confusion_matrix_result )\n",
    "\n",
    "\n",
    "tn, fp, fn, tp = confusion_matrix_result .ravel()\n",
    "\n",
    "\n",
    "accuracy = (tp + tn) / (tp + tn + fp + fn)\n",
    "precision_pos = tp / (tp + fp)\n",
    "recall_pos = tp / (tp + fn)\n",
    "f1_pos = 2 * (precision_pos * recall_pos) / (precision_pos + recall_pos)\n",
    "\n",
    "precision_neg = tn / (tn + fn)\n",
    "recall_neg = tn / (tn + fp)\n",
    "f1_neg = 2 * (precision_neg * recall_neg) / (precision_neg + recall_neg)\n",
    "\n",
    "print(\"Accuracy:\", accuracy)\n",
    "print(\"Precision (Positive Class):\", precision_pos)\n",
    "print(\"Recall (Positive Class):\", recall_pos)\n",
    "print(\"F1-Score (Positive Class):\", f1_pos)\n",
    "\n",
    "print(\"Precision (Negative Class):\", precision_neg)\n",
    "print(\"Recall (Negative Class):\", recall_neg)\n",
    "print(\"F1-Score (Negative Class):\", f1_neg)"
